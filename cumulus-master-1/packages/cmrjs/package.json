{
  "name": "@cumulus/cmrjs",
  "version": "2.0.1",
  "description": "A node SDK for CMR",
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "clean": "git clean -d -x -e node_modules -f",
    "prepare": "npm run tsc",
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test",
    "tsc": "../../node_modules/.bin/tsc",
    "watch-test": "../../node_modules/.bin/tsc-watch --onsuccess 'npm test'"
  },
  "ava": {
    "serial": true,
    "timeout": "15m"
  },
  "keywords": [
    "CUMULUS"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/cmrjs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus",
    "directory": "packages/cmrjs"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/aws-client": "2.0.1",
    "@cumulus/cmr-client": "2.0.1",
    "@cumulus/common": "2.0.1",
    "@cumulus/errors": "2.0.1",
    "@cumulus/launchpad-auth": "2.0.1",
    "got": "^8.3.0",
    "js2xmlparser": "^4.0.0",
    "lodash": "^4.17.15",
    "public-ip": "^3.0.0",
    "url-join": "^1.0.0",
    "xml2js": "^0.4.19"
  }
}
